{
    "name": "express-vue-mvc-starter",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "test": "npm run lint && NODE_ENV=development nyc ava",
        "lint": "tslint --project tsconfig.json --fix",
        "build": "npm run sass",
        "copy": "ncp app dist",
        "sass": "node-sass --output-style compressed app/assets/scss/app.scss app/assets/rendered/style.css",
        "start": "nodemon --inspect=127.0.0.1:9230 app/index.js",
        "preversion": "npm run test",
        "version": "git add .",
        "postversion": "git push --follow-tags"
    },
    "ava": {
        "files": [
            "test/**/*.js"
        ],
        "source": [
            "**/*.js",
            "!dist/**/*"
        ],
        "concurrency": 5,
        "failFast": true,
        "powerAssert": false,
        "require": [
            "babel-register"
        ],
        "babel": "inherit"
    },
    "nyc": {
        "include": [
            "app/**/*.js"
        ],
        "reporter": [
            "text-summary"
        ]
    },
    "dependencies": {
        "@types/mongoose": "^5.3.3",
        "@types/tail": "^1.2.0",
        "@types/uuid": "^3.4.4",
        "acorn": "^6.0.4",
        "always-tail": "^0.2.0",
        "babel-cli": "^6.26.0",
        "babel-plugin-istanbul": "^4.1.6",
        "babel-register": "^6.26.0",
        "body-parser": "^1.18.3",
        "bootstrap-vue": "^2.0.0-rc.11",
        "browserify": "^16.2.3",
        "bulma": "^0.7.2",
        "compression": "^1.7.3",
        "cookie-parser": "^1.3.3",
        "cookie-session": "^2.0.0-beta.3",
        "csurf": "^1.9.0",
        "dotenv": "^5.0.0",
        "express": "^4.16.4",
        "express-session": "^1.15.6",
        "express-validator": "^5.3.0",
        "express-vue": "^5.8.2",
        "fs-web": "^1.0.1",
        "glob": "^7.1.3",
        "global": "^4.3.1",
        "gulp-ava": "^0.19.0",
        "helmet": "^3.15.0",
        "i18n": "^0.8.3",
        "jquery": "^3.3.1",
        "jsonwebtoken": "^8.4.0",
        "method-override": "^2.3.10",
        "mongoose": "^5.3.14",
        "morgan": "^1.9.1",
        "node-monkey": "^1.0.0-beta.5",
        "node-sass": "^4.11.0",
        "npm": "^6.4.1",
        "serve-favicon": "^2.5.0",
        "tail": "^2.0.1",
        "vue-dropdowns": "^1.0.6",
        "vue-read-file": "^1.0.2"
    },
    "devDependencies": {
        "@types/compression": "0.0.36",
        "@types/cookie-parser": "^1.4.1",
        "@types/cookie-session": "^2.0.36",
        "@types/dotenv": "^4.0.3",
        "@types/express": "^4.16.0",
        "@types/glob": "^5.0.36",
        "@types/helmet": "0.0.37",
        "@types/jsonwebtoken": "^7.2.8",
        "@types/method-override": "0.0.31",
        "@types/morgan": "^1.7.35",
        "ava": "^0.24.0",
        "axios": "^0.18.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.6",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-node6": "^11.0.0",
        "css-loader": "^1.0.1",
        "del": "^3.0.0",
        "eslint": "^4.19.1",
        "eslint-plugin-ava": "^4.5.1",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.1",
        "gulp-concat": "^2.6.1",
        "gulp-coveralls": "^0.1.4",
        "gulp-eslint": "^4.0.2",
        "gulp-file-cache": "0.0.1",
        "gulp-livereload": "^3.8.1",
        "gulp-nodemon": "^2.4.2",
        "gulp-notify": "^3.2.0",
        "gulp-nsp": "^3.0.0",
        "gulp-sass": "^3.2.1",
        "gulp-sourcemaps": "^2.6.4",
        "isparta": "^4.1.0",
        "ncp": "^2.0.0",
        "nyc": "^11.9.0",
        "request": "^2.88.0",
        "rimraf": "^2.6.2",
        "time-grunt": "^1.2.1",
        "tslint": "^5.11.0",
        "typescript": "^2.9.2",
        "uuid": "^3.3.2",
        "vue-loader": "^15.4.2",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.17",
        "webpack": "^4.27.1",
        "webpack-cli": "^3.1.2"
    },
