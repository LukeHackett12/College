window = 1;

for accuracy = [1, 3, 5, 10, 50, 500]
    subplot(3, 2, window)
    title("Approximation with " + accuracy + " sine functions");
    
    hold on;
    
    x = 1:1:accuracy;
    k = 1;
    tempY = 0;
    for i = 1:accuracy
        %y is amplitude and x is sin wave 1 to accuracy 
        y = sin(2 * pi * k * x)/k;
        k = k + 2;
    end
    
    window = window + 1;
end